library(network)
library(sna)
library(ggplot2)
library(GGally)
edges_input <- 'sample_edges.csv'
labels_input <- 'sample_labels.csv'
pdf('sample_graph.pdf')
vertices <- read.csv(labels_input, sep=",", header=F)
edges <- read.csv(edges_input, sep=",", header=F)
net <- network.initialize(length(vertices[,1]))
net <- add.edges(net, edges[,1], edges[,2])
ggnet2(net, color=vertices[,3], mode="fruchtermanreingold", size=3, palette="Set2")
dev.off()
